{
  "name": "repo-sync",
  "productName": "Repo Sync",
  "authors": "marhano",
  "description": "Github project management app",
  "repository": {
    "type": "git",
    "url": "https://github.com/marhano/repo-sync"
  },
  "version": "0.0.3",
  "main": "main.js",
  "scripts": {
    "electron": "ng build --base-href ./ && electron .",
    "ng": "ng",
    "start": "electron-forge start",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:repo-sync": "node dist/repo-sync/server/server.mjs",
    "start:dev": "ng serve --configuration=development",
    "start:prod": "ng serve --configuration=production",
    "start:ssr": "node dist/repo-sync/server/server.mjs",
    "start:web": "concurrently \"ng build\" \"npm run start:ssr\" \"npm run start\"",
    "start:electron": "ng build --base-href ./ && concurrently \"npm run start:ssr\" \"npm run electron\"",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish"
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "^19.2.8",
    "@angular/common": "^19.2.0",
    "@angular/compiler": "^19.2.0",
    "@angular/core": "^19.2.0",
    "@angular/forms": "^19.2.0",
    "@angular/material": "^19.2.8",
    "@angular/platform-browser": "^19.2.0",
    "@angular/platform-browser-dynamic": "^19.2.0",
    "@angular/router": "^19.2.0",
    "@angular/ssr": "^19.2.6",
    "axios": "^1.8.4",
    "body-parser": "^2.2.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "electron-squirrel-startup": "^1.0.1",
    "express": "^5.1.0",
    "marked": "^15.0.7",
    "ngx-cookie-service": "^19.1.2",
    "ngx-markdown": "^19.1.1",
    "ngx-mat-select-search": "^8.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "update-electron-app": "^3.1.1",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.6",
    "@angular/cli": "^19.2.6",
    "@angular/compiler-cli": "^19.2.0",
    "@electron-forge/cli": "^7.8.0",
    "@electron-forge/maker-deb": "^7.8.0",
    "@electron-forge/maker-rpm": "^7.8.0",
    "@electron-forge/maker-squirrel": "^7.8.0",
    "@electron-forge/maker-zip": "^7.8.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.0",
    "@electron-forge/plugin-fuses": "^7.8.0",
    "@electron-forge/publisher-github": "^7.8.0",
    "@electron/fuses": "^1.8.0",
    "@types/axios": "^0.9.36",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^18.18.0",
    "concurrently": "^9.1.2",
    "electron": "^35.1.4",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.7.2"
  }
}
